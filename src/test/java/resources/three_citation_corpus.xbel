        <bel:statementGroup>
            <bel:annotationGroup>
                <bel:annotation bel:refID="CardiovascularSystem">Arteries</bel:annotation>
                <bel:evidence>Arterial cells are
highly susceptible to oxidative stress, which can induce
both necrosis and apoptosis (programmed cell death) [1,2].</bel:evidence>
            </bel:annotationGroup>
            <bel:statement bel:relationship="increases">
