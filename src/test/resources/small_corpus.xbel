        <bel:statementGroup>
            <bel:name>Abstract</bel:name>
            <bel:annotationGroup>
                <bel:annotation bel:refID="TextLocation">Abstract</bel:annotation>
                <bel:annotation bel:refID="Cell">Keratinocytes</bel:annotation>
            </bel:annotationGroup>
            <bel:statementGroup>
                <bel:annotationGroup>
                    <bel:evidence>The epidermis develops depending on the transcription factor p63, a member of the p53 family of transcription factors.</bel:evidence>
                </bel:annotationGroup>
                <bel:statement bel:relationship="increases">
                    <bel:annotationGroup />
                    <bel:subject>
                        <bel:term bel:function="transcriptionalActivity">
                            <bel:term bel:function="proteinAbundance">
