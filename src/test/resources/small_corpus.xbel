                    </bel:annotationGroup>
                    <bel:subject>
                        <bel:term bel:function="microRNAAbundance">
                            <bel:parameter bel:ns="MGI">Mir203</bel:parameter>
                        </bel:term>
                    </bel:subject>
                    <bel:object>
                        <bel:term bel:function="biologicalProcess">
